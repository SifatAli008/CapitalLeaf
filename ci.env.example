# CI/CD Environment Configuration for CapitalLeaf
# This file contains environment variables for automated testing and deployment

# Server Configuration
NODE_ENV=test
PORT=3000

# Database Configuration (Test Environment)
MONGODB_URI=mongodb://localhost:27017/capitalleaf_test
REDIS_URL=redis://localhost:6379/1

# Security Configuration (Test Keys - Never use in production)
JWT_SECRET=test-jwt-secret-key-for-ci-cd-pipeline
ENCRYPTION_KEY=test-encryption-key-for-ci-cd-pipeline

# Threat Intelligence (Test Mode)
THREAT_INTELLIGENCE_ENABLED=true
THREAT_FEED_API_KEY=test-threat-feed-api-key

# DLP Configuration (Test Mode)
DLP_ENABLED=true
DLP_SENSITIVE_KEYWORDS=test,account,balance,credit,debit,transaction,payment

# CI/CD Specific
CI=true
VERCEL_ENV=test
